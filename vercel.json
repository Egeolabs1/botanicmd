{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/(manifest\\.json|sw\\.js|icon\\.svg)",
      "destination": "/$1"
    },
    {
      "source": "/:file(\\..+)$",
      "destination": "/:file"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

